    "params":[
        { 
           "name": "dataset_tag",
           "from": "submitter",
           "type": "text",
           "default": "CrIS_VIIRS_Matchup",
           "placeholder": "A value to append to the dataset identifier"
        },
        {
           "name":"end_time",
           "from":"submitter"
        },
        {
           "name":"start_time",
           "from":"submitter"
        },
        {
           "name": "input_metadata",
           "type": "text",
           "from": "dataset_jpath:_source.metadata"
        },
        {
           "name":"localize_products",
           "from": "dataset_jpath:_source.metadata.localize_urls"
        }
    ]
}
